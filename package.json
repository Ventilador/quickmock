{
  "name": "quickmock",
  "version": "1.0.11",
  "description": "quickmock is an simple service for automatically injecting mocks into your AngularJS unit tests using Jasmine",
  "main": "src/quickmock.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ../Proteus/Proteus-GUI/node_modules/quickmock/built && babel src --presets babel-preset-es2015 --out-dir ../Proteus/Proteus-GUI/node_modules/quickmock/built && webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tennisgent/QuickMock.git"
  },
  "keywords": [
    "AngularJS",
    "Testing",
    "Jasmine",
    "quickmock",
    "Unit Testing"
  ],
  "author": "Zack Boman <tennisgent@gmail.com> (https://github.com/tennisgent)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tennisgent/QuickMock/issues"
  },
  "homepage": "https://github.com/tennisgent/QuickMock",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0"
  }
}